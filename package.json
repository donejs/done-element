{
  "name": "done-element",
  "version": "0.2.0",
  "description": "Custom elements for DoneJS projects",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/donejs/done-element.git"
  },
  "author": {
    "name": "DoneJS Contributors",
    "email": "contact@donejs.com",
    "url": "https://donejs.com/"
  },
  "scripts": {
    "preversion": "npm test",
    "version": "git commit -am \"Update version number\"",
    "postpublish": "git push --tags && git push",
    "testee": "testee test.html --browsers firefox",
    "test": "npm run jshint && npm run testee",
    "jshint": "jshint ./*.js --config",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "develop": "done-serve --static --develop --port 8080"
  },
  "main": "done-element.js",
  "browser": {
    "transform": [
      "cssify"
    ]
  },
  "browserify": {
    "transform": [
      "cssify"
    ]
  },
  "keywords": [
    "Donejs",
    "custom",
    "elements",
    "donejs-plugin"
  ],
  "steal": {
    "configDependencies": [
      "./node_modules/steal-conditional/conditional.js"
    ]
  },
  "dependencies": {
    "@webcomponents/custom-elements": "webcomponents/custom-elements#v1.0.0-alpha.4",
    "can-element": "^0.2.0",
    "cloudydom": "^1.0.1"
  },
  "devDependencies": {
    "can-define": "^2.7.18",
    "can-stache": "^4.17.19",
    "can-stache-bindings": "^4.10.8",
    "done-serve": "^3.3.1",
    "donejs-cli": "^3.2.2",
    "generator-donejs": "^3.4.0",
    "jshint": "^2.9.1",
    "steal": "^2.2.1",
    "steal-conditional": "^1.1.3",
    "steal-qunit": "^2.0.0",
    "steal-tools": "^2.2.2",
    "testee": "^0.9.1"
  },
  "license": "MIT"
}
